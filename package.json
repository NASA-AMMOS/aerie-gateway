{
  "name": "aerie-gateway",
  "description": "The API gateway for Aerie.",
  "version": "2.20.0",
  "type": "module",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/NASA-AMMOS/aerie-gateway.git"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.6.4"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf dist",
    "dev": "npm run watch & npm run dev-server",
    "dev-server": "nodemon --watch dist --delay 3 --exec 'NODE_TLS_REJECT_UNAUTHORIZED=0 node -r dotenv/config dist/main.js'",
    "format": "prettier --write ./src ./test",
    "format:check": "prettier --check ./src ./test",
    "lint": "eslint ./src --ext .js,.ts",
    "start": "node dist/main.js",
    "test": "vitest run",
    "watch": "nodemon --watch src --ext .js,.ts --exec 'tsc'"
  },
  "dependencies": {
    "@streamparser/json": "^0.0.21",
    "altair-express-middleware": "^5.2.11",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^4.0.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.1",
    "swagger-jsdoc": "^7.0.0-rc.6",
    "swagger-ui-express": "^4.6.3",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/pg": "^8.10.2",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6",
    "vitest": "^1.2.2"
  }
}
