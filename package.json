{
  "name": "aerie-gateway",
  "version": "0.10.0",
  "type": "module",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.jpl.nasa.gov/Aerie/aerie-gateway.git"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf dist",
    "hasura:metadata:apply": "hasura metadata apply",
    "hasura:metadata:apply:local": "hasura metadata apply --envfile .env.local",
    "hasura:metadata:export": "hasura metadata export",
    "hasura:metadata:export:local": "hasura metadata export --envfile .env.local",
    "start": "node -r dotenv/config dist/main.js dotenv_config_path=./.env",
    "start:local": "node -r dotenv/config dist/main.js dotenv_config_path=./.env.local"
  },
  "dependencies": {
    "@gov.nasa.jpl.aerie/cam": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "fast-glob": "^3.2.7",
    "graphql-playground-middleware-express": "^1.7.22",
    "helmet": "^4.6.0",
    "multer": "^1.4.3",
    "nanoid": "^3.1.28",
    "pg": "^8.7.1",
    "swagger-jsdoc": "^7.0.0-rc.6",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/nanoid": "^3.0.0",
    "@types/pg": "^8.6.1",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "prettier": "^2.4.1",
    "hasura-cli": "^2.0.9",
    "typescript": "^4.4.3"
  }
}
